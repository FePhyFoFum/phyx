OPT_FLAGS = -O3 -ffast-math -ftree-vectorize -fopenmp -g -std=c++0x
#profiling 
#-g -pg
#-O3 -ffast-math -ftree-vectorize 

# Each subdirectory must supply rules for building sources it contributes
%.o: ./%.cpp
	g++ $(OPT_FLAGS) -c -fmessage-length=0 -MMD -MP -MF"$(@:%.o=%.d)" -MT"$(@:%.o=%.d)" -o  "$@" "$<"

# All Target
all: pxstrec pxmrca pxmrcacut pxmrcaname pxconseq pxbd pxnni pxseqtest pxstofa pxstophy pxstonex pxfqfilt pxrevcomp pxnw pxsw pxboot pxcontrates pxseqrecode pxbp pxbpseq

